Cell["\<\

numberDivisors[x_] :=Length[Divisors[x]]; 

divisorSum[f_,n_]:=
  Total[Map[f,Divisors[n]]];

sumDigitsBase[b_, n_] :=
Sum[IntegerDigits[n,b][[k]],{k,1,Length[IntegerDigits[n,b]]}];
zerosBase[b_,n_]:=Count[IntegerDigits[n,b],0];
     
listSumDigitsBase[b_, lst_] := 
  Module[{ln, answer, elt, i}, len = Length[lst]; answer = {}; 
    For[i = 1, i < len + 1, i++, 
     elt = lst[[i]]; AppendTo[answer, sumDigitsBase[b, elt]]; ]; 
     answer];
     
 numberGhostDivisors[x_] := 
  Module[{llist, sm, n, len}, llist = FactorInteger[x]; 
  len = Length[llist]; 
    sm = 0; 
    For[n = 1, n < len + 1, n++, sm = sm + llist[[n]][[2]]; Return[2^sm]]];
    
 roundOdd[x_] := 
Module[{y}, y = Floor[x]; If[Mod[y, 2] == 0, y = y + 1]; y];

ord[p_,r_]:=
\tModule[{d,f},
\t\tIf[r==0,Return[\\[Infinity]]];
\td=0;
\t\tWhile[
\t\t\tMod[Numerator[r],p^d]==0,d=d+1;
\t\t\t           ];
\t\tf=0;
\t\t\tWhile[
\t\t\tMod[Denominator[r],p^f]==0,f=f+1;
\t\t\t           ];
\t\td-f
\t\t];
\t\t
\t\t
unit2adic[n_]:=n/2^ord[2,n]
\
